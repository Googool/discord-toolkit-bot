{
  "name": "discord-toolkit-bot",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "watch": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": "biome check --write --no-errors-on-unmatched"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/bun": "latest",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5"
  }
}
